// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource postgresql {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model User {
  id       String @id @default(uuid()) @postgresql.Uuid
  name     String @default("")
  email    String @unique @default("")
  password String
}

model Experience {
  id               String    @id @default(uuid()) @postgresql.Uuid
  title            String    @default("")
  slug             String    @unique @default("")
  poster_filesize  Int?
  poster_extension String?
  poster_width     Int?
  poster_height    Int?
  poster_mode      String?
  poster_id        String?
  startDate        DateTime?
  endDate          DateTime?
  status           String?   @default("draft")
  content          Json      @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  artworks         Artwork[] @relation("Experience_artworks")
}

model Artwork {
  id                       String       @id @default(uuid()) @postgresql.Uuid
  title                    String       @default("")
  artist                   String       @default("")
  slug                     String       @unique @default("")
  startDate                DateTime?
  endDate                  DateTime?
  status                   String?      @default("draft")
  spotifyUrl               String       @default("")
  soundcloudUrl            String       @default("")
  audioFile_filesize       Int?
  audioFile_mode           String?
  audioFile_filename       String?
  images_filesize          Int?
  images_extension         String?
  images_width             Int?
  images_height            Int?
  images_mode              String?
  images_id                String?
  overview                 Json         @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  from_Experience_artworks Experience[] @relation("Experience_artworks")
}